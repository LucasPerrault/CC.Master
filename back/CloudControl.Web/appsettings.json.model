{
    "Logging": {
        "LogLevel": {
            "Default": "Error"
        },
        "Debug": {
            "LogLevel": {
                "Default": "Warning"
            }
        }
    },
    "SqlInfos": {
        "Default": "Data Source={{ CC.Master:SqlServer }};Initial Catalog=CloudControl;user={{ CC.Master:SqlLogin }};Password={{ CC.Master:SqlPassword }};MultipleActiveResultSets=True;App=CloudControl"
    },
    "Authentication": {
        "ServerUri": "{{ CC.Master:PartenairesUri }}",
        "UsersEndpointPath": "{{ CC.Master:AuthUsersEndpointPath }}",
        "ApiKeysEndpointPath": "{{ CC.Master:AuthApiKeysEndpointPath }}",
        "RedirectEndpointPath": "{{ CC.Master:AuthRedirectEndpointPath }}",
        "LogoutEndpointPath": "{{ CC.Master:AuthLogoutEndpointPath }}",
        "ApiKeysFetcherToken": "{{ CC.Master:AuthApiKeysFetcherToken }}"
    },
    "Rights": {
        "ServerUri": "{{ CC.Master:PartenairesUri }}",
        "DepartmentsEndpointPath": "{{ CC.Master:RightsDepartmentEndpointPath }}",
        "ForeignAppEndpointPath": "{{ CC.Master:RightsForeignAppEndpointPath }}",
        "UsersEndpointPath": "{{ CC.Master:RightsUsersEndpointPath }}"
    },
    "Redis": {
        "Host": "{{ shared:CacheManagerRedisHost }}",
        "Password": "{{ shared:CacheManagerRedisPassword }}",
        "KeyInvalidationInMinutes": "720"
    },
    "LuccaLoggerOptions": {
        "ApplicationName": "CloudControl",
        "ConnectionString": "{{ shared:LuccaLogsConnectionString }}",
        "LogFilePath": "{{ CC.Master:LogFilePath }}",
        "EventIdSeparator": "eventId",
        "IgnoreEmptyEventId": "false"
    },
    "LuccaSecurity": {
        "IpWhitelist": {
            "ResponseStatusCode": 401,
            "AuthorizedIPAddresses": "{{ CC.Master:WhitelistedIps }}"
        }
    },
    "LegacyCloudControl": {
        "Host": "{{ CC.Master:LegacyCloudControlHost }}"
    },
    "BillingContracts": {
        "LegacyClientsEndpointPath": "/api/v3/clients"
    },
    "Salesforce": {
        "ServerUri": "{{ CC.Master:SalesforceUri }}",
        "AccountsEndpointPath": "/salesforce/accounts",
        "Token": "{{ CC.Master:SalesforceToken }}"
    },
    "Instances": {
        "InstancesStore": {
            "Host": "{{ CC.Master:LegacyCloudControlHost }}",
            "Endpoint": "/api/v3/instances",
            "Token": "{{ CC.Master:LegacyCommunicationToken }}"
        },
        "Identity": {
            "ClientId": "{{ luccaidentity:CloudControlClientId }}",
            "ClientSecret": "{{ luccaidentity:CloudControlClientSecret }}",
            "TokenRequestRoute": "/identity/connect/token"
        },
        "WsAuth": {
            "ServerUri": "https://ws-auth.ilucca.local",
            "EndpointPath": "/sync-v2/api",
            "Token": "{{ Lucca.Auth.Sync:CloudControlToken }}"
        },
        "CcData": {
            "Domain": "lucca.local",
            "Scheme": "http",
            "OutboundToken": "{{ CC.Data:CCMasterWebserviceToken }}",
            "InboundToken": "{{ CC.Data:CCMasterWebserviceToken }}"
        },
        "Hubspot": {
            "ServerUri": "{{ CC.Master:HubspotServerUri }}",
            "OutboundToken": "{{ CC.Master:HubspotOutboundToken }}"
        },
        "SqlScriptPicker": {
            "JenkinsBaseUri": "http://jenkins2.lucca.local:8080/",
            "MonolithJobPath": "job/CI.Pipeline.Multibranch"
        },
        "DemoClusterSelection": {
            "UseFixedCluster": "{{ CC.Master:UseFixedClusterForDemos }}",
            "FixedClusterName": "{{ CC.Master:FixedClusterName }}",
            "ClusterChoiceCacheRetentionInHours": 24
        }
    }
}
